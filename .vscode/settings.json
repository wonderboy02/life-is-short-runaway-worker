{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a5775b",
    "activityBar.background": "#a5775b",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#98c7aa",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#a5775b",
    "statusBar.background": "#845f49",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#a5775b",
    "statusBarItem.remoteBackground": "#845f49",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#845f49",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#845f4999",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#845f49"
}
